<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zt.entities.Log" table="LOGS">
		<id name="id" column="ID" type="java.lang.String">
			<generator class="uuid"></generator>
		</id>

		<property name="operator" type="java.lang.String">
			<column name="OPERATOR" />
		</property>

		<property name="operName" type="java.lang.String">
			<column name="OPER_NAME" />
		</property>

		<property name="operParams" type="java.lang.String">
			<column name="OPER_PARAMS" />
		</property>

		<property name="operResult" type="java.lang.String">
			<column name="OPER_RESULT" />
		</property>

		<property name="resultMsg" type="java.lang.String">
			<column name="RESULT_MSG" />
		</property>

		<property name="operTime" type="java.util.Date">
			<column name="OPER_TIME" />
		</property>
	</class>
</hibernate-mapping>
